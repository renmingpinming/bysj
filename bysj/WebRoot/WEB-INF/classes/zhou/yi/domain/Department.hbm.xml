<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="zhou.yi.domain.Department" table="department">
		<id name="did" column="did">
			<generator class="native"/>
		</id>
		<property name="dname" column="dname" length="20"></property>
		<property name="ddesc" column="ddesc"></property>
		<property name="password" column="password" length="20"></property>
		
	</class>
</hibernate-mapping>