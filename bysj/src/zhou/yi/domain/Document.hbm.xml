<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="zhou.yi.domain.Document" table="document">
		<id name="id" column="id">
			<generator class="native"/>
		</id>
		<property name="filename" column="filename" length="20"></property>
		<property name="course_id" column="course_id" length="12"></property>
		<property name="hid" column="hid" length="12"></property>

	</class>
</hibernate-mapping>